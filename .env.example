# AI Data Lakehouse - Environment Configuration
# Copy this file to .env and customize as needed

# --- RavenDB ---
RAVENDB_URL=http://localhost:8080
RAVENDB_PORT=8080
RAVENDB_TCP_PORT=38888

# --- MinIO (S3-Compatible Storage) ---
MINIO_ENDPOINT=http://localhost:9100
MINIO_PORT=9100
MINIO_CONSOLE_PORT=9101
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=password
AWS_REGION=us-east-1

# --- Nessie (Iceberg Catalog) ---
NESSIE_URL=http://localhost:19120/api/v1
NESSIE_PORT=19120

# --- Milvus (Vector Database) ---
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_METRICS_PORT=9091

# --- Spark ---
SPARK_UI_PORT=4040
SPARK_JUPYTER_PORT=8888

# --- Dremio (SQL Query Engine) ---
DREMIO_UI_PORT=9047
DREMIO_CLIENT_PORT=31010
DREMIO_FLIGHT_PORT=32010
DREMIO_USER=admin
DREMIO_PASSWORD=password